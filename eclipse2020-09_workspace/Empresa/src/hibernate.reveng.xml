<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-reverse-engineering PUBLIC "-//Hibernate/Hibernate Reverse Engineering DTD 3.0//EN" "http://hibernate.org/dtd/hibernate-reverse-engineering-3.0.dtd" >

<hibernate-reverse-engineering>
  <table-filter match-schema="empresa" match-name=".*"/>
  <table-filter match-catalog="empresa" match-name="departamento"/>
  <table-filter match-catalog="empresa" match-name="empleado"/>
  <table-filter match-catalog="empresa" match-name="empleado_datos_prof"/>
  <table-filter match-catalog="empresa" match-name="proyecto"/>
  <table-filter match-catalog="empresa" match-name="proyecto_sede"/>
  <table-filter match-catalog="empresa" match-name="sede"/>
</hibernate-reverse-engineering>